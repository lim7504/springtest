<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

	<typeAliases>
		<typeAlias type="com.spring.sample.board.BoardVO" alias="board"></typeAlias>
		<typeAlias type="com.spring.sample.user.UserVO" alias="user"></typeAlias>
